[user]
  name = [NAME HERE]
  email = [EMAIL HERE]
[core]
  editor = subl -w
  autocrlf = input
  safecrlf = warn
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short -100
  type = cat-file -t
  dump = cat-file -p
[credential]
  helper = osxkeychain
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = cyan
  changed = yellow
  untracked = red
[push]
  default = current
[help]
  autocorrect = 1
